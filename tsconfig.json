{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/e2e",
    "module": "commonjs",
    "target": "es2018",
    "types": [
      "node",
      "webdriverio/async",
      "@wdio/jasmine-framework",
      "wdio-image-comparison-service"
    ],
    "paths": {
      "@config/*": [
        "./config/*"
      ],
      "@enums/*": [
        "./enums/*"
      ],
      "@pages/*": [
        "./pages/*"
      ],
      "@type/*": [
        "./types/*"
      ],
    }
  }
}
